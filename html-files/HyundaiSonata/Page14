      <script>

        function clearEmptyFilterField() {
            CG.Utils.checkEmptyField('fullTextSearchInput', '(eg. diesel, sunroof)');
        }

        function emptyFullTextSearchFilterField() {
            $('#fullTextSearchInput').val("").blur();
            fullTextSearchFilter.clearSearch();
            fullTextSearchFilter.handleChange();
        }

        $(document).ready(function() {
            CG.Utils.initEmptyField('fullTextSearchInput', '(eg. diesel, sunroof)');
            $('i.s2b_tooltip').tooltip({
              "html" : true,
              "placement" : "right",
              "template" : "<div class=\"tooltip cg-dealFinder-textFilterTooltip\" role=\"tooltip\"><div class=\"tooltip-arrow\"></div><div class=\"tooltip-inner\"></div></div>"
            });
        });

        function submitFullTextSearchFilterQuery() {
            var query = $('#fullTextSearchInput').val();

            if (query == null || query == "" || query.trim() == "") {
                alert("Please enter one or more search terms to filter these listings.");
                $('#fullTextSearchInput').blur();
                return;
            }

            // show searching clue
            $('#fullTextSearchLoadingImage').show();
            $('#searchInputButtons').hide();

            var successCallback = function () {
                $('#fullTextSearchLoadingImage').hide();
                $('#searchInputButtons').show();
            };

            var errorCallback = function() {
                $('#fullTextSearchLoadingImage').hide();
                $('#searchInputButtons').show();

                alert("Sorry no matching results!");
            };

            fullTextSearchFilter.performSearch(query, successCallback, errorCallback, false);
        }

      </script>

      <script>
      $(document).ready(function() {
          var results_container       = $('#filteredOutAllListingResultsMessageContainer'),
                  listings_container      = $('#listingsContent');

          $(window).scroll(function(){
              var bannerAdHeight = $('#bannerAdLEADERBOARD_INLINE_1-container').outerHeight() || 0;
              var topOffset = $('#searchResultsContainer2').position().top + bannerAdHeight;
              var bottomOffset = listings_container.offset().top + listings_container.height() - results_container.height();
              var scrollTop = $(window).scrollTop();
              if ( topOffset <=  scrollTop &&  scrollTop <= bottomOffset ) {
                  results_container.addClass('sticky');
              } else {
                  results_container.removeClass('sticky');
              }
          });
          listingsSearchManager.initializeInlineBannerAds();
        });
      </script>

