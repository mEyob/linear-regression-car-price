  function deleteSavedSearch(id, label) {
    var successCB = function () {
      var deepest = $('#saveSearchButton *:not(:has("*"))');
      $("#shopperListingsSearchId").val("");  // clear it
      $("#savedSearchLink" + id).remove();
      $("#saveSearchButton").removeClass("disabled");
      $("#saveSearchButton").show();
      $("#activeFiltersWrap #savedSearchLabel").hide();

      $("#subscribeToDealsButton").removeClass("disabled");
      $("#subscribeToDealsButton").show();

      deepest.html("Save this search");
      $('.deleteSearchLink').hide();

      var savedSearchesCount = $('#savedSearchDropdownList li').length;
      if (savedSearchesCount == 0) {
        $("#savedSearchDropdown").html('<span id="savedSearchesDropDown">No Saved Searches</span>');
        $("#savedSearchDropdown").addClass('disabled');
        $('#cg-menu-savedSearch').html("");
      } else {
        $('#cg-menu-savedSearch').html(savedSearchesCount);
      }
    };

    CG.MySavedSearches.deleteSavedSearch(id, label, successCB);
  }


