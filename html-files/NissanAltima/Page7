      '/Cars/inventorylisting/viewPromptConvertSearchToSubscriptionDialog.action');

    listingsListingDealsSubscription_overview.entityId = 'd237';
    listingsListingDealsSubscription_overview.entityId2 = '';


    listingsListingDealsSubscription_overview.isMobile = false;

    listingsListingDealsSubscription_overview.emailSelector = '#listingsSubscriptionEmail_overview';

