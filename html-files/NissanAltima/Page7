      listingsSearchManager.registerPageViewHandler(function () {

        if (!$.bbq.getState("resultsPage") && !$.isEmptyObject($.bbq.getState())) {
          $(".cg-siteHeader-leaderBoardWrap").show();
        } else {
          $(".cg-siteHeader-leaderBoardWrap").hide();
        }

        if ((typeof cgDataLayer === 'object' && cgDataLayer !== null) && !listingsSearchManager.isOnVdp()) {
          cgDataLayer.resetPageData();
          listingsSearchManager.refreshCustomInfo();
        }

        CG.pageTracker.trackPageview('/Cars/l-Used-Nissan-Altima-Quincy-d237_L15709');
      });

      $(document).ready(function () {
        $("#addModelsToSearchResultsPopup").hide();


        $(document).on('shoppedListingCountChange', function (event, shopper) {
          if (shopper && shopper != null) {
            CG.MySavedListings.updateSavedListingsLabel(shopper.savedListingCount);
          }
        });

