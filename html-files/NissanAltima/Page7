var saveSearchOnclick_overview = function() {
listingsListingDealsSubscription_overview.trims = [];
listingsListingDealsSubscription_overview.trimLevels = [];
listingsListingDealsSubscription_overview.styles = [];
var toReturn = listingsListingDealsSubscription_overview.saveSearchOnClick(listingsSearchManager);
return toReturn;
};

function saveSearchAndSubscription_overview(email, useSavedSearch) {
    listingsListingDealsSubscription_overview.subscribeEmail = email;
    listingsListingDealsSubscription_overview.skipPopupPrompts = true;
    listingsListingDealsSubscription_overview.subscribeToListingsSearch(false, listingsSearchManager, "_DEFAULT_");
}

var saveSearchOnclick = function() {
saveSearchOnclick_overview();
}

function saveSearchAndSubscription(email, useSavedSearch) {
saveSearchAndSubscription_overview(email, useSavedSearch);
}
