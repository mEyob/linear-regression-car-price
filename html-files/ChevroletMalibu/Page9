      <script>
      $(window).on('CG.Adjust.Zip.UK', function () {
        // In UK, tracking just the service provider's postal code could skew analytic traffic,
        // So keep regional part of service provider postal code, but since those chars are insufficient
        // for a new search, users will be forced to complete it or put in their own full postal code.
        // This only applies if there is no zip already associated with the user
          if (typeof CG === 'object' &&
              CG !== null &&
              CG.hasOwnProperty('Inventory') &&
              false) {
                var postalCodeInput = $('.cg-postalCode');
                var userPostalCode = '02169';

                if (userPostalCode) {
                  postalCodeInput.val(userPostalCode);
                } else if (CG.isUK() && postalCodeInput.length > 0) {
                  var formattedDealerPostalCode = postalCodeInput.val().split(" ")[0];
                  postalCodeInput.val(formattedDealerPostalCode);
                }
          }
      })

      $(document).trigger('CG.Adjust.Zip.UK');
      </script>

