      <script>
        function handleSeoReadMoreLink() {
          var seoReadMore = document.querySelector('.seo-read-more');
          var readMoreButton = document.querySelector('.btn-read-more');

          if (readMoreButton) {
            readMoreButton.onclick = function(e) {
              e.preventDefault();

              if (seoReadMore.classList.contains('expanded')) {
                seoReadMore.classList.remove('expanded');
                readMoreButton.innerHTML = 'Read More';
              } else {
                seoReadMore.classList.add('expanded');
                readMoreButton.innerHTML = 'Read Less';
              }
            }
          }
        }

        if (document.readyState === 'complete' || (document.readyState !== 'loading' && !document.documentElement.doScroll)) {
          handleSeoReadMoreLink();
        } else {
          document.addEventListener('DOMContentLoaded', handleSeoReadMoreLink);
        }
            </script>

